<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/widget28"
	android:layout_width="320dip" android:layout_height="140dip"
	android:background="@drawable/background" xmlns:android="http://schemas.android.com/apk/res/android">
	<TextView android:id="@+id/freqRange" android:text="@string/freqRangeEurope"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
		android:textSize="18sp" android:layout_width="250dip"
		android:layout_height="50dip" android:paddingBottom="20dip">
	</TextView>
	<EditText android:id="@+id/txtFrequency" android:text="@string/empty"
		android:textSize="18sp" android:layout_below="@+id/freqRange"
		android:layout_marginLeft="50dip" android:layout_width="120dip"
		android:layout_height="50dip">
	</EditText>
	<TextView android:id="@+id/label" android:layout_width="wrap_content"
		android:text="@string/label" android:layout_alignBottom="@+id/txtFrequency"
		android:textSize="20sp" android:layout_height="40dip"
		android:layout_marginLeft="190dip">
	</TextView>
	<Button android:layout_width="162dip" android:layout_height="40dip"
		android:text="@string/btnOk" android:layout_below="@+id/txtFrequency"
		android:layout_alignParentLeft="true" android:id="@+id/btnOk"
		android:paddingTop="5dip">
	</Button>
	<Button android:id="@+id/btnCancel" android:layout_width="162dip"
		android:layout_height="40dip" android:text="@string/btnCancel"
		android:layout_alignParentRight="true" android:layout_below="@+id/txtFrequency"
		android:layout_alignRight="@+id/btnOk" android:paddingTop="5dip">
	</Button>
</RelativeLayout>
